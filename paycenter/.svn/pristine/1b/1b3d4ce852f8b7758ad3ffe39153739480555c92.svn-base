}
/* 必填选项标记*/
.cl:after,
.clearfix:after {
  content: "\20";
  display: block;
  height: 0;
  clear: both;
  visibility: hidden;
}
.cl,
.clearfix {
  zoom: 1;
}
.f-l {
  float: left;
}
.f-r {
  float: right;
}
.text-readonly {
  background: #ebebe4;
}
.radius-6 {
  -webkit-border-radius: 0.08rem;
  -moz-border-radius: 0.08rem;
  border-radius: 0.08rem;
}
.radius-circle {
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  border-radius: 50%;
}
.sf_btn {
  display: inline-block;
  cursor: pointer;
  height: 1.2rem;
  line-height: 1.2rem;
  font-size: 0.472rem;
