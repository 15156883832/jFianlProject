}
.main .btn-more {
  position: absolute;
  right: 0.28rem;
  top: 50%;
  margin-top: -0.6rem;
  line-height: 1.24rem;
  height: 1.24rem;
  padding-right: 0.6rem;
  color: #9c9c9c;
}
.main .btn-more:after {
  content: '';
  position: absolute;
  top: 50%;
  right: 0.28rem;
  margin-top: -0.17333333333333334rem;
  height: 0.32rem;
  width: 0.32rem;
  border: solid #c5c5c5;
  border-width: 0 2px 2px 0;
  -webkit-transform: rotate(-45deg);
  -moz-transform: rotate(-45deg);
  -ms-transform: rotate(-45deg);
  transform: rotate(-45deg);
}
.main .icon-arrow2-up,
.main .icon-arrow2-down {
  display: inline-block;
  height: 0.16rem;
  width: 0.24rem;
  position: relative;
}
.main .icon-arrow2-up:before,
.main .icon-arrow2-down:before {
  content: '';
  position: absolute;
  left: 0;
  width: 0;
  height: 0;
  border-style: solid;
  border-width: 0.16rem 0.12rem;
  border-left-color: transparent;
  border-right-color: transparent;
}
.main .icon-arrow2-up:before {
  bottom: 0;
  border-top-color: transparent;
  border-bottom-color: #888;
}
.main .icon-arrow2-down:before {
  top: 0;
  border-top-color: #888;
  border-bottom-color: transparent;
}
.main .arrow-active .icon-arrow2-up:before {
  border-bottom-color: #ff1203;
}
.main .arrow-active .icon-arrow2-down:before {
  border-top-color: #ff1203;
}
