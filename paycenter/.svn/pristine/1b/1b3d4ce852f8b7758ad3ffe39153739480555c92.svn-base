html {
  height: 100%;
  overflow: hidden;
}
body {
  margin: 0 auto;
  font-family: Helvetica, 'STHeiti', 'Microsoft YaHei', Arial, sans-serif;
  color: #2a2a2a;
  position: relative;
  overflow: hidden;
  height: 100%;
  -webkit-text-size-adjust: 100%!important;
}
body * {
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
}
h1,
h2,
h3,
h4,
p,
ul,
li,
dl,
dd,
dt,
input,
textarea {
  margin: 0;
  padding: 0;
}
ul,
li,
dl,
dd,
dt {
  list-style: none;
}
h1,
h2,
h3,
h4,
strong {
  font-weight: normal;
}
a,
a:active,
a:hover,
a:visited {
  text-decoration: none;
}
a,
input,
button,
optgroup,
select,
textarea {
  -webkit-tap-highlight-color: rgba(255, 0, 0, 0);
}
a,
img {
  -webkit-touch-callout: none;
  /* 禁止长按链接与图片弹出菜单 */

}
img {
  border: none;
  vertical-align: top;
}
input,
textarea,
button,
optgroup,
select {
  border: none;
  outline: none;
  -webkit-appearance: none;
}
textarea {
  resize: none;
  overflow: auto;
}
i,
em {
  font-style: normal;
}
label {
  font-weight: normal;
}
input::-webkit-input-placeholder {
  color: #9c9c9c;
}
body {
  max-width: 10rem;
}
.bodybg {
  background-color: #ededed;
}
.fc-fd1520 {
